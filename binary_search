#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int n;
    printf("Enter the length of an array");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        arr[i]= rand()%10+1;
    }
    for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }

    clock_t t;
    t=clock();
    int count=0;
    int search=rand()%10+1;


    for(int i = 0; i < n; i++) {
        for (int j = 0; j < n - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    printf("\nsorted array\n");
    for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }

    int low=0,high=n-1;
    int found =0;
    while (low <= high) {
        int mid = low + (high - low) / 2;
        if (arr[mid] == search){
            printf("\nFound %d", search);
            found=1;
            break;}

        else if (arr[mid] < search)
            low = mid + 1;
        else
            high = mid - 1;
        count+=1;
    }

    if (found!=1)
        printf("\nNot found");

    t=clock()-t;
    double time_taken =((double)t)/CLOCKS_PER_SEC;
    printf("\n%f",time_taken);
    printf("\n count = %d",count);
    return 0;


}

OUTPUT
Enter the length of an array11
8 10 4 9 1 3 5 9 4 10 1 
sorted array
1 1 3 4 4 5 8 9 9 10 10 
Not found
0.000021
 count = 3
Process finished with exit code 0
